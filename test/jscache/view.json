{
  "type": "view",
  "layout": "horizontal",
  "items": [{
      "type": "view",
      "layout": "vertical",
      "items": [{
        "type": "toolbar",
        "items": [{
          "bind": "addTreeBtn",
          "type": "button",
          "onClick": "PartnerList.add"
        }, {
          "bind": "editTreeBtn",
          "type": "button",
          "onClick": "PartnerList.edit"
        }, {
          "bind": "deleteTreeBtn",
          "type": "button",
          "onClick": "PartnerList.delete"
        }]
      } ]
    }
  ]
}
